{
  "name": "ProfessionalLight",
  "dark": false,
  "author": "QLVeTau Team",
  "ui": {
    "@accentColor": "#1976D2",
    "@accentColorHover": "#0D47A1",
    "@controlBackground": "#FFFFFF",
    "@background": "#F5F5F5",
    "@textColor": "#212121",
    "@disabledText": "#9E9E9E",
    "@borderColor": "#BDBDBD",
    "@selectionBackground": "#E3F2FD",
    "@selectionForeground": "#0D47A1",
    
    "Button.arc": 6,
    "Component.arc": 6,
    "TextComponent.arc": 6,
    "CheckBox.arc": 6,
    "ProgressBar.arc": 6,
    
    "Button.background": "@accentColor",
    "Button.foreground": "#FFFFFF",
    "Button.hoverBackground": "@accentColorHover",
    "Button.pressedBackground": "#0D47A1",
    "Button.focusedBackground": "@accentColor",
    "Button.disabledBackground": "#E0E0E0",
    "Button.disabledText": "@disabledText",
    "Button.default.background": "@accentColor",
    "Button.default.foreground": "#FFFFFF",
    "Button.default.hoverBackground": "@accentColorHover",
    "Button.default.focusedBackground": "@accentColor",
    "Button.borderWidth": 0,
    
    "TabbedPane.selectedBackground": "#E3F2FD",
    "TabbedPane.selectedForeground": "#1976D2",
    "TabbedPane.hoverColor": "#E0E0E0",
    "TabbedPane.underlineColor": "@accentColor",
    "TabbedPane.unselectedForeground": "#757575",
    "TabbedPane.tabInsets": "6,12,6,12",
    "TabbedPane.tabHeight": 40,
    "TabbedPane.contentBorderInsets": "0,0,0,0",
    
    "TableHeader.background": "#2196F3",
    "TableHeader.foreground": "#FFFFFF",
    "TableHeader.separatorColor": "#1976D2",
    "TableHeader.height": 32,
    "TableHeader.cellBorder": "0,8,0,8",
    
    "Table.background": "#FFFFFF",
    "Table.foreground": "@textColor",
    "Table.selectionBackground": "@selectionBackground",
    "Table.selectionForeground": "@selectionForeground",
    "Table.gridColor": "#E0E0E0",
    "Table.rowHeight": 28,
    "Table.showHorizontalLines": true,
    "Table.showVerticalLines": false,
    "Table.intercellSpacing": "0,1",
    
    "ScrollBar.thumb": "#BDBDBD",
    "ScrollBar.hoverThumbColor": "#9E9E9E",
    "ScrollBar.pressedThumbColor": "#757575",
    "ScrollBar.track": "#F5F5F5",
    "ScrollBar.thumbArc": 6,
    "ScrollBar.trackArc": 6,
    "ScrollBar.width": 12,
    
    "TextField.background": "@controlBackground",
    "TextField.foreground": "@textColor",
    "TextField.caretForeground": "@accentColor",
    "TextField.selectionBackground": "@selectionBackground",
    "TextField.selectionForeground": "@selectionForeground",
    "TextField.inactiveForeground": "@disabledText",
    "TextField.disabledBackground": "#FAFAFA",
    "TextField.placeholderForeground": "@disabledText",
    
    "TextArea.background": "@controlBackground",
    "TextArea.foreground": "@textColor",
    "TextArea.caretForeground": "@accentColor",
    "TextArea.selectionBackground": "@selectionBackground",
    "TextArea.selectionForeground": "@selectionForeground",
    
    "ComboBox.background": "@controlBackground",
    "ComboBox.foreground": "@textColor",
    "ComboBox.selectionBackground": "@selectionBackground",
    "ComboBox.selectionForeground": "@selectionForeground",
    "ComboBox.buttonBackground": "@controlBackground",
    "ComboBox.buttonArrowColor": "@textColor",
    
    "Panel.background": "@background",
    "Panel.foreground": "@textColor",
    
    "Label.foreground": "@textColor",
    "Label.disabledForeground": "@disabledText",
    
    "Menu.background": "@controlBackground",
    "Menu.foreground": "@textColor",
    "Menu.selectionBackground": "@selectionBackground",
    "Menu.selectionForeground": "@selectionForeground",
    
    "MenuItem.background": "@controlBackground",
    "MenuItem.foreground": "@textColor",
    "MenuItem.selectionBackground": "@selectionBackground",
    "MenuItem.selectionForeground": "@selectionForeground",
    
    "MenuBar.background": "@background",
    "MenuBar.foreground": "@textColor",
    
    "PopupMenu.background": "@controlBackground",
    "PopupMenu.foreground": "@textColor",
    "PopupMenu.borderColor": "@borderColor",
    
    "CheckBox.icon.checkmarkColor": "@accentColor",
    "CheckBox.icon.borderColor": "@borderColor",
    "CheckBox.icon.disabledBorderColor": "@disabledText",
    "CheckBox.icon.focusedBorderColor": "@accentColor",
    "CheckBox.icon.selectedBorderColor": "@accentColor",
    "CheckBox.icon.selectedBackground": "@accentColor",
    
    "RadioButton.icon.checkmarkColor": "@accentColor",
    "RadioButton.icon.borderColor": "@borderColor",
    "RadioButton.icon.disabledBorderColor": "@disabledText",
    "RadioButton.icon.focusedBorderColor": "@accentColor",
    "RadioButton.icon.selectedBorderColor": "@accentColor",
    "RadioButton.icon.selectedBackground": "@accentColor",
    
    "ProgressBar.background": "#E0E0E0",
    "ProgressBar.foreground": "@accentColor",
    "ProgressBar.selectionForeground": "#FFFFFF",
    "ProgressBar.selectionBackground": "@textColor"
  }
}
